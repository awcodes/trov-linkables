{
    "name": "awcodes/trov-linkables",
    "type": "project",
    "description": "Linkable Sets Resource for integration into Trov CMS (for SEO purposes)",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "trov",
        "cms",
        "faq"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "awcodes/trov": "^0.2.12",
        "awcodes/trov-components": "^0.3.0",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Trov\\Linkables\\": "src/",
            "Trov\\Linkables\\Database\\Factories\\": "database/factories/",
            "Trov\\Linkables\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trov\\Linkables\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trov\\Linkables\\TrovLinkablesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}